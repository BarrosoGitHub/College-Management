model IEnumerable<CollegeManagement.Models.Course>

<h1>Courses</h1>

@foreach (var course in Model)
{
    <h2>@course.Name</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Subject Id</th>
                <th>Subject Name</th>
                <th>Professor Name</th>
                <th>Students</th>
            </tr>
        </thead>
        <tbody>
        @if (course.CourseSubjects != null)
        {
            @foreach (var courseSubject in course.CourseSubjects)
            {
                <tr>
                    <td>@courseSubject.Subject?.Id</td>
                    <td>@courseSubject.Subject?.Name</td>
                    <td>@courseSubject.Subject?.Professor?.Name</td>
                    <td>
                        <a href="/Students?courseId=@course.Id&subjectId=@courseSubject.Subject?.Id">View Students</a> <!-- Add this line -->
                    </td>
                </tr>
            }
        }
        </tbody>
    </table>
}